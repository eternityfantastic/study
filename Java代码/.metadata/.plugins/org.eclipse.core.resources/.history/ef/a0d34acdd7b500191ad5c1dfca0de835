package com.young.test;

import java.lang.reflect.Method;

public class Test06 {
	//阶乘
	int method01(int num)
	{
		int result=1;
		for (int i = 1; i <= num; i++) {
			result*=i;
		}
		return result;
	}
	public static void main(String[] args) {
		double e;
		for (int i = 0; i < 1000; i++) {
			e+=1.0/method01(i);
			
		}
	}

}
